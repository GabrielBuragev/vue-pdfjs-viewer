  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielBuragev/vue-pdfjs-viewer.git"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "rollup:dev": "NODE_ENV=development rollup -c",
