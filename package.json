{
  "name": "vue_pdfjs_viewer",
  "version": "0.1.6",
  "main": "dist/vue_pdfjs_viewer.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-bundle": "vue-cli-service build --target lib --name vue_pdfjs_viewer src/index.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
